{
  "name": "spruce",
  "version": "6.1.54",
  "private": true,
  "type": "module",
  "scripts": {
    "bootstrap-s3-logs": "../../scripts/bootstrap-s3-logs.sh",
    "build:beta": "env-cmd -e beta pnpm build",
    "build:local": "env-cmd -e local pnpm build",
    "build:prod": "env-cmd -e production pnpm build",
    "build:staging": "env-cmd -e staging pnpm build",
    "build": "GIT_SHA=`git rev-parse HEAD` REACT_APP_VERSION=$npm_package_version vite build",
    "build:profiler:beta": "PROFILER=true PROFILE_HEAD=\"<script src=\"https://localhost:8097\"></script>\" pnpm build:beta",
    "build:profiler:local": "PROFILER=true PROFILE_HEAD=\"<script src=\"https://localhost:8097\"></script>\" pnpm build:local",
    "build:profiler:staging": "PROFILER=true PROFILE_HEAD=\"<script src=\"https://localhost:8097\"></script>\" pnpm build:staging",
    "check-schema-and-codegen": "env-cmd -e local vite-node scripts/check-schema-and-codegen/script.ts",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf node_modules && rm -rf dist && pnpm install",
    "codegen": "graphql-codegen-esm --config graphql.config.ts",
    "cy:open": "pnpm evg-db-ops --reseed-and-dump; cypress open",
    "cy:run": "pnpm evg-db-ops --reseed-and-dump; cypress run",
    "cy:test": "pnpm evg-db-ops --reseed-and-dump; cypress run --spec",
    "deploy:beta": "env-cmd -e beta pnpm deploy-utils-deploy",
    "deploy:staging": "env-cmd -e staging pnpm deploy-utils-deploy",
    "deploy:prod": "env-cmd -e production pnpm deploy-utils-deploy",
    "dev": "env-cmd -e devLocal pnpm start",
    "eslint:fix": "pnpm eslint:strict --fix",
    "eslint:staged": "STRICT=1 eslint --concurrency=auto",
    "eslint:strict": "STRICT=1 eslint --max-warnings=61 --concurrency=auto .",
    "evg-db-ops": "../../scripts/evg-db-ops.sh",
    "postversion": "pnpm deploy-utils-postversion",
    "prepare": "pnpm --dir ../.. prepare",
    "preview": "serve -s dist -p 3000",
    "prod": "env-cmd -e devProduction pnpm start",
    "snapshot": "vitest run snapshot.test.ts",
    "staging": "env-cmd -e devStaging pnpm start",
    "start": "vite",
    "storybook": "NO_HTTPS=true storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "vitest --exclude=src/snapshot.test.ts",
    "chromatic": "npx chromatic --project-token=chpt_676fe75e2c97368 --build-script-name storybook:build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
