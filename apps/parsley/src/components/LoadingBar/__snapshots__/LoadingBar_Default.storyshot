  border-radius: 4px;
  width: 100%;
  overflow: hidden;
}

.emotion-Bar {
  border-top-left-radius: 4px;
  border-bottom-left-radius: 4px;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
  height: 6px;
  background-color: #00ED64;
  width: 100%;
  box-shadow: 0 0 8px #C0FAE6;
}

